--		
--FTP download/upload
--
ftpTable OBJECT-TYPE
	SYNTAX SEQUENCE OF FtpEntry 
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
   		" The FTP client parameters table."
	::= { ftp 1 }
		
ftpEntry OBJECT-TYPE
		SYNTAX FtpEntry 
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
	   		"An entry in the table.  "
		INDEX { ftpRequesterID }
		::= { ftpTable 1 }
	
FtpEntry ::=
	SEQUENCE { 
		ftpRequesterID 
			IpAddress,
		ftpFileIdentifier 
			INTEGER,
		ftpFileName 
			DisplayString,
		ftpFileServer 
			IpAddress,
     	ftpStatus
			INTEGER, 
		ftpRowStatus 
			RowStatus,
		ftpClientDirection 
			INTEGER,
		ftpUserName 
			DisplayString,
		ftpPassword
			DisplayString 
	 }
	
ftpRequesterID OBJECT-TYPE
	SYNTAX IpAddress
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
     	"The download/upload requester IP Address." 
 	::=  { ftpEntry 1 }
	
ftpFileIdentifier OBJECT-TYPE
	SYNTAX INTEGER {
			 software(1),
		     backupData(2),
		     logfile(3)
   	}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
 		"The identifier of the type of file to be uploaded / downloaded."
 	::=  { ftpEntry 2 }
			
ftpFileName OBJECT-TYPE
	SYNTAX DisplayString(SIZE(1..32))
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
      	"The Name of the file to be up/downloaded."
  	::=  { ftpEntry 3 }
			
ftpFileServer OBJECT-TYPE
	SYNTAX IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
      	"The IP Address of the up/download file server."
 	::=  { ftpEntry 4 }
	
ftpStatus  OBJECT-TYPE
    SYNTAX  INTEGER {
                success(1),
                inProgress(2),
                failed(3)
            }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
   		"This object contains the current status of the 
	    last requested up/download of the corresponding
		  row component."
   ::= { ftpEntry 5 }

ftpRowStatus OBJECT-TYPE
	SYNTAX RowStatus
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"The row status variable."
	::= { ftpEntry 6 }
	
ftpClientDirection OBJECT-TYPE
	SYNTAX INTEGER {
           	  download (1),
			upload (2)
   	}
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
 			" The behavior of the FTP machine when Client Mode is selected."
    DEFVAL {1}
  	::=  { ftpEntry 7 }
                                                                                             
ftpUserName OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS  read-create
	STATUS  current
	DESCRIPTION
	  	" FTP User Name when Client Mode is selected."
	::=  { ftpEntry 8 }

ftpPassword OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS  read-create
	STATUS  current
	DESCRIPTION
   		" FTP password when Client Mode is selected."
 	::=  { ftpEntry 9 }            
 	
--
--Alarm--
--        
ctxAlarmTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF CtxAlarmEntry 
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
	   "This table contains alarms raised."
	::= { ctxAlarms	1 }

ctxAlarmEntry OBJECT-TYPE
	SYNTAX      CtxAlarmEntry 
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
	   "Entries appear in this table once alarms are raised."
	INDEX     { ctxAlarmIndex }
	::= {ctxAlarmTable 1 }

CtxAlarmEntry ::= SEQUENCE {     
			ctxAlarmIndex  					INTEGER,
			ctxAlarmIfIndex  				IfIndex,
			ctxAlarmType     				AlarmType,
			ctxAlarmSeverity				INTEGER,   
			ctxAlarmServiceAffect 			INTEGER,   
			ctxAlarmActive					INTEGER,
			ctxAlarmText	     			DisplayString 
		}   
		
ctxAlarmIndex  OBJECT-TYPE
	SYNTAX      INTEGER
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "Alarm index"
	::= { ctxAlarmEntry 1 }  

ctxAlarmIfIndex    	OBJECT-TYPE
	SYNTAX      IfIndex
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "Interface index"
	::= { ctxAlarmEntry 2 }           
	
ctxAlarmType    	OBJECT-TYPE
	SYNTAX      AlarmType
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "Alarm type:    
  		psuFailure(1)--  电源故障，2个电源板卡
		psuInserted(2)--  电源插入
		psuPlugged(3)--    电源拔出
		psuLowVolt(4)--  电压过低
		chassisTemp(5)--  机箱温度告警
		fanCardPlugged(6)--  风扇板卡拔出
		fanCardInserted(7)--  风扇板卡插入
		fanFailure(8)--     风扇故障（三个风扇）
		swUpgFailure(9)--   SW升级失败告警
		sntpFailure(10)--    SNTP协议失败
		lofsonetAlarm(11)--   LOF (Loss of Frame),Loss of sync for SONET/SDH Service.
		aisLineSonetAlarm(12)-- AIS-L (Line Alarm Indication Signal)/MS-AIS (MS AlarmIndication Signal)，Alarm Indication Signal (AIS) has been detected on the SONET/SDH or 10GbE-WAN link.
		RfiLineSonetAlarm(13)-- RFI-L (Line Remote Failure Indication)/MS-RFI (MS Remote Failure Indication，Remote Failure Indication (RFI)has been detected on the SONET/SDH or 10GbE-WAN link.
		ethConfigLinkFail(14)-- Ethernet Link Failure, Ethernet link failure. Check the Auto Negotiation parameters.
		ethConfigPcsLossSync(15)--	Loss of Synchronization, Loss of sync for Ethernet Service. 			
		fcBxPortLossOfSync(16)--  Loss of Synchronization, Loss of sync for FC Service.
		portLinkUp(17)--  Standard Trap; Sent when the link status changed to UP.
		portLinkDown(18)-- Standard Trap; Sent when the link status is changed to DOWN.
		opticModuleRxOverload(19)-- 光模块Rx接收光功率过载.
		opticModuleRxLow(20)--  光模块Rx输入功率过低.
		opticModuleLossOfInputPower(21)--  光模块Rx接收无光.
		opticModuleTxTooLow(22)--  光模块Tx发射功率过低.
		opticModuleTxTooHigh(23)--  光模块Tx发射功率过高.
		opticModuleTemp(24)--       光模块温度告警.
		opticModuleTxBiasCurrent(25)--   光模块Tx Current告警.
		opticModuleRemoved(26)--   光模块拔掉了.
		opticModuleBitRateMismatch(27)-- 光模块速率与端口速率不匹配告警.
		opticModuleLossOfLock(28)--   Signal Loss of Lock.
		opticModuleBlock(29)--       Unauthorized optic inserted非认证光模块插入(Tx关断).
		opticModuleTxLossOfLock(30)--Signal Transmission Loss of Lock.				
		otnFecExc(31)--     OTN Excessive FEC Corrected Errors ，FEC纠错前误码越限告警 .
		otnFecDeg(32)--     ON Degraded FEC Corrected Errors 
		otnOtuDeg(33)--     OTN Section Degrade，OTUk/ ODUk PM/ ODUk TCMi级别信号劣化. 
		otnLos(34)--        OTN LOS.
		otnLof(35)--        OTN LOF, OTUk帧丢失，帧对齐处理连续3ms处于帧失步状态则上报此告警.
		otnLom(36)--        OTN Loss of Multiframe, OTUk复帧丢失，复帧定位处理过程连续3ms处于复帧失步状态，即MFAS开销为非法值.
		otuAis(37)--        OTN Section AIS, OTUk告警指示信号，整个OTUk为PN-11序列通用AIS信号，指示上游检测到的信号失效.
		otuBdi(38)--        OTN Section BDI, OTUk/ ODUk PM/ ODUk TCMi级别后向缺陷指示告警.
		otuTtim(39)--       OTN Section Trace Mismatch ，OTUk/ ODUk PM/ ODUk TCMi级别踪迹标识不匹配，本站相应级别应收和实收的TTI通道追踪字节不一致.
		otnOtuEXC(40)--     OTN Section Excessive Errors，泊松模式下，OTUk/ ODUk PM/ ODUk TCMi级别BIP8误码率超过设定的误码越限门限，主要告警.
		oduAis(41)--        OTN Path AIS, ODUk PM / ODUk TCMi告警指示信号，检测AIS，看相应的STAT开销是否为111.
		oduLofLom(42)--     ODU Path LOFLOM, ODUk帧和复帧丢失，上下游的帧头信息不匹配，帧定位处理过程持续帧失步状态3ms.
		oduOci(43)--        OTN Path Open Connection , ODUk PM/ ODUk TCMi级别断开连接指示告警，相应级别接收的STAT信息是110时产生OCI告警，指示输出没有连接到输入。
		oduLck(44)--        OTN Path Locked ，ODUk PM/ ODUk TCMi级别信号锁定缺陷.
		oduBdi(45)--        OTN Path BDI, OTUk/ ODUk PM/ ODUk TCMi级别后向缺陷指示告警.
		oduTtim(46)--       OTN Path Trace ，OTUk/ ODUk PM/ ODUk TCMi级别踪迹标识不匹配，本站相应级别应收和实收的TTI通道追踪字节不一致Mismatch 
		otnOduDeg(47)--     OTN Path Degrade，OTUk/ ODUk PM/ ODUk TCMi级别信号劣化.
		oduPtm(48)--        OTN Path Payload Mismatch. 
		otnOduEXC(49)--	    OTN Path Excessive Errors , 泊松模式下，OTUk/ ODUk PM/ ODUk TCMi级别BIP8误码率超过设定的误码越限门限，主要告警.
	   "
	::= { ctxAlarmEntry 3 }

ctxAlarmSeverity    	OBJECT-TYPE
	SYNTAX      INTEGER{   
					noAlarm(1),
					major(2), 
					minor(3),
					cleared(4), 
					event(5) 
	}
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "noAlarm(1) 
		major(2) - Major Severity 
		minor(3) - Minor Severity 
		cleared(4) - the alarm is cleared 
		event(5)"
	::= { ctxAlarmEntry 4 }

ctxAlarmServiceAffect    	OBJECT-TYPE
	SYNTAX      INTEGER{ 
				true(1), 
				false(2)
	}
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "true(1) - service affecting 
		false(2) - non service affecting"
	::= { ctxAlarmEntry 5 }

ctxAlarmActive    	OBJECT-TYPE
	SYNTAX      INTEGER
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "true(1) - the alarm is active 
		false(2) - the alarm is cleared"
	::= { ctxAlarmEntry 6 }

ctxAlarmText    	OBJECT-TYPE
	SYNTAX      DisplayString
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	   "Always empty."
	::= { ctxAlarmEntry 7 }

