upgFirmware OBJECT-TYPE
		SYNTAX  INTEGER{  
					inprogress(1),
					finished(2),                                                                                                                                                                                                                                                                            
					failed(3)
		}
		ACCESS  read-only
		STATUS  current
		DESCRIPTION
			"升级软件//系统维护类：in progress，finished, failed."
		::= { ctxNodeinfo 13 }    
		

	    		
 
