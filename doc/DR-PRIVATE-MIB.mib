--
--SeriveType
--
ServiceType ::= TEXTUAL-CONVENTION
    STATUS         current
    DESCRIPTION    "The type of service."
    SYNTAX         INTEGER {
							otu4(1),											--100G OTN
							gbe100lan(2),										--100G以太网
							gbe10lan(3),										--10G以太网
							oc192(4),											--10G Sonet
							stm64(5),											--10G SDH
							gbe10wan(6),										--10G WAN
							otu2(7),											--10G OTN
							otu2e(8),											--10G OTN
							fc8g(9),											--8G FC光纤通道
							fc10g(10)											--10G FC光纤通道
    }                 
    
IfIndex ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "d"
    STATUS       current
    DESCRIPTION
            "A unique value, greater than zero, for each interface or
            interface sub-layer in the managed system. "
    SYNTAX       Integer32 (1..2147483647)
    
AlarmType ::= TEXTUAL-CONVENTION
    STATUS         current
    DESCRIPTION    "The type of ctxAlarm."
    SYNTAX         INTEGER {
				psuFailure(1),
				psuInserted(2),
				psuPlugged(3),
				psuLowVolt(4),
				chassisTemp(5),
				fanCardPlugged(6),
				fanCardInserted(7),
				fanFailure(8),
				swUpgFailure(9),
				sntpFailure(10),
				lofsonetAlarm(11),
				aisLineSonetAlarm(12),
				rfiLineSonetAlarm(13),
				ethConfigLinkFail(14),
				ethConfigPcsLossSync(15),				
				fcBxPortLossOfSync(16),
				portLinkUp(17),
				portLinkDown(18),
				opticModuleRxOverload(19),
				opticModuleRxLow(20),
				opticModuleLossOfInputPower(21),
				opticModuleTxTooLow(22),
				opticModuleTxTooHigh(23),
				opticModuleTemp(24),
				opticModuleTxBiasCurrent(25),
				opticModuleRemoved(26),
				opticModuleBitRateMismatch(27),
				opticModuleLossOfLock(28),
				opticModuleBlock(29),
				opticModuleTxLossOfLock(30),				
				otnFecExc(31),
				otnFecDeg(32),
				otnOtuDeg(33), 
				otnLos(34),
				otnLof(35),
				otnLom(36),
				otuAis(37),
				otuBdi(38),
				otuTtim(39),
				otnOtuEXC(40),
				oduAis(41),
				oduLofLom(42),
				oduOci(43),
				oduLck(44),
				oduBdi(45),
				oduTtim(46),
				otnOduDeg(47), 
				oduPtm(48),
				otnOduEXC(49)							
    }                  
--	
--Chassisinfo
--
ctxChassisinfoTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CtxChassisinfoEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
   		"机框信息表."
	::= { ctxChassisinfo 1 }     
		
ctxChassisinfoEntry OBJECT-TYPE
	SYNTAX CtxChassisinfoEntry 
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
   		"An entry in the table.  "
	INDEX { chassisIndex }
	::= { ctxChassisinfoTable 1 }
	
CtxChassisinfoEntry ::=
	SEQUENCE { 
		chassisIndex 
			INTEGER,
		ctxPN 
			DisplayString,
		ctxchassisID 
			INTEGER,
		ctxPSU1 
			INTEGER,
		ctxPSU2 
			INTEGER,
		ctxTEMP 
			INTEGER,
		ctxLowTempAlmThreshold 
			INTEGER,
		ctxHighTempAlmThreshold 
			INTEGER,
		ctxFAN 
			INTEGER
	 }

chassisIndex OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS  not-accessible
	STATUS  current
	DESCRIPTION
 			"Chassis index." 
	::=  { ctxChassisinfoEntry 1 }
	
ctxPN  OBJECT-TYPE
	SYNTAX  DisplayString    
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION
	    " 机箱型号Part Number CTX2000"
  	::= { ctxChassisinfoEntry 2 }
  
ctxchassisID   OBJECT-TYPE 
	  SYNTAX  INTEGER    
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      "机箱编号"
	  ::= { ctxChassisinfoEntry 3 }		
  
ctxPSU1  OBJECT-TYPE
	  SYNTAX  INTEGER{
	                normal(1),
					failure(2),
					exist(3),
					notexist(4),
					inserted(5),
					plugged(6)	            }    
	  MAX-ACCESS read-only
	  STATUS     current
	  DESCRIPTION
	      " 电源1"
	  ::= { ctxChassisinfoEntry 4 }		
  
ctxPSU2  OBJECT-TYPE
  SYNTAX  INTEGER{
                normal(1),
				failure(2),
				exist(3),
				notexist(4),
				inserted(5),
				plugged(6)
        }        
  MAX-ACCESS read-only
  STATUS     current
  DESCRIPTION
      "电源2"
  ::= { ctxChassisinfoEntry 5 }		

ctxTEMP  OBJECT-TYPE
	  SYNTAX  INTEGER{
	                emp(1),
					high(2),
					low(3)
	            }           
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      "机箱温度Celsius"
	  ::= { ctxChassisinfoEntry 6 }		

ctxLowTempAlmThreshold  OBJECT-TYPE
	  SYNTAX  INTEGER    
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      "机箱内低温告警门限"
	  ::= { ctxChassisinfoEntry 7 }		

ctxHighTempAlmThreshold  OBJECT-TYPE
	  SYNTAX  INTEGER    
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      "机箱内高温告警门限"
	  ::= { ctxChassisinfoEntry 8 }		
  
ctxFAN  OBJECT-TYPE
	  SYNTAX  INTEGER{
	                normal(1),
					plugged(2),
					inserted(3),
					fan1fault(4),
					fan2fault(5),
					fan3fault(6)
         }           
	  MAX-ACCESS read-only
	  STATUS     current
	  DESCRIPTION
	      "风扇"
	  ::= { ctxChassisinfoEntry 9 }		

--
--Nodeinfo
--
