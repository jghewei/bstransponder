--
--Optic Ports 
--
ctxPortConfigTable OBJECT-TYPE
		SYNTAX SEQUENCE OF CtxPortConfigEntry 
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
	   		" MXP port table."
		::= { ctxPortManage 1 }
	
ctxPortConfigEntry OBJECT-TYPE
		SYNTAX CtxPortConfigEntry 
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
    		"An entry in the table.  "
		INDEX { ctxPortIndex }
		::= { ctxPortConfigTable 1 }
		
CtxPortConfigEntry ::=
		SEQUENCE { 
			ctxPortIndex
				IfIndex,
			ctxPortType 
				DisplayString,
			ctxPortRate 
				DisplayString,
			ctxAdminStatus 
				INTEGER,
			ctxOperationalStatus 
				INTEGER,   
			ctxPortOptics
			    INTEGER,   
			ctxServiceType 
				ServiceType,   
			ctxPortDataFlow
				INTEGER,				
			ctxGccInband 
				INTEGER,
			ctxPortAlias 
				DisplayString 
}

ctxPortIndex  OBJECT-TYPE
	  SYNTAX  IfIndex       
	  MAX-ACCESS not-accessible
	  STATUS     current
	  DESCRIPTION
	      "端口号."
	  ::= { ctxPortConfigEntry 1 }		

ctxPortType  OBJECT-TYPE
	  SYNTAX  DisplayString
