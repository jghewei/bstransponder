--
--Diagnostic and Test
--
ctxPortDiagnosticTable OBJECT-TYPE
		SYNTAX SEQUENCE OF CtxPortDiagnosticEntry 
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
    		"Diagnostic and Test table."
		::= { ctxPortManage 2 }
	
ctxPortDiagnosticEntry OBJECT-TYPE
		SYNTAX CtxPortDiagnosticEntry 
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION
    		"	An entry in the table.  "
		INDEX { opticPortNum }
		::= { ctxPortDiagnosticTable 1 }
		
CtxPortDiagnosticEntry ::=
		SEQUENCE { 
			ctxPortNum 
				IfIndex,
			ctxLoopTestDuration 
				INTEGER,
			ctxLoopTestStatus 
				INTEGER,
			ctxLoopTestType 
				INTEGER,
			ctxLoopTestMode 
				INTEGER,
			ctxLoopTestErrors 
				INTEGER,
			ctxLoopTestResults 
				INTEGER,
			ctxLoopTestPassSecond 
				INTEGER
}

ctxPortNum  OBJECT-TYPE
	  SYNTAX  IfIndex    
	  MAX-ACCESS not-accessible
	  STATUS     current
	  DESCRIPTION
	      "端口号."
	  ::= { ctxPortDiagnosticEntry 1 }		

ctxLoopTestDuration  OBJECT-TYPE
	  SYNTAX  INTEGER    
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      " "
	  ::= { ctxPortDiagnosticEntry 2 }		

ctxLoopTestStatus  OBJECT-TYPE
	  SYNTAX  INTEGER{
				start(1),
				stop(2),
				fail(3)
		}    
	  MAX-ACCESS read-write
	  STATUS     current
	  DESCRIPTION
	      "Start or Stop a test."
	  ::= { ctxPortDiagnosticEntry 3 }		

ctxLoopTestType   OBJECT-TYPE
	  SYNTAX  INTEGER{
						erminal(1),
						facility(2),
						prbs(3),
						otnPrbs(4)
		}    
